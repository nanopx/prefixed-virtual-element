{
  "name": "prefixed-virtual-element",
  "version": "0.0.1",
  "description": "Build virtual tree elements with class attributes and vendor-prefixed styles",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel index.js --out-dir lib",
    "prepublish": "npm run clean && npm run build",
    "preversion": "npm run clean && npm run test",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "deku",
    "vendor prefix",
    "autoprefixer",
    "userAgent"
  ],
  "author": "nanopx",
  "license": "MIT",
  "dependencies": {
    "component-type": "^1.2.0",
    "deku": "~2.0.0-rc",
    "inline-style-prefixer": "^0.6.3",
    "magic-virtual-element": "git+https://github.com/dekujs/magic-virtual-element.git"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "rimraf": "^2.5.0"
  }
}
